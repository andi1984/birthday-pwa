{
    "version": 2,
    "name": "birthday-pwa",
    "alias": ["www.andi1984.social", "andi1984.social"],
    "regions": ["bru1", "cdg1"],
    "builds": [
        { "src": "next.config.js", "use": "@now/next" },
        { "src": "server/**/*.js", "use": "@now/node" }
    ],
    "build": {
        "env": {
            "SENTRY_KEY": "@sentry-key",
            "MY_AWS_ACCESS_KEY_ID": "@aws-id",
            "MY_AWS_SECRET_ACCESS_KEY": "@aws-secret"
        }
    }
}
